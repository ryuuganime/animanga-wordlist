{
    "python.analysis.typeCheckingMode": "strict",
    "python.formatting.autopep8Args": [
        "--max-line-length",
        "80",
        "--in-place",
        "--aggressive",
        "--aggressive"
    ],
    "python.linting.pylintArgs": [
        "--init-hook",
        "import sys; sys.path.insert(0, '.')"
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "python.formatting.provider": "autopep8"
}
